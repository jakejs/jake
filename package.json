{
  "name": "jake",
  "description": "JavaScript build tool, similar to Make or Rake",
  "keywords": [
    "build",
    "cli",
    "make",
    "rake"
  ],
  "version": "8.1.1",
  "author": "Matthew Eernisse <mde@fleegix.org> (http://fleegix.org)",
  "license": "Apache-2.0",
  "bin": {
    "jake": "./bin/cli.js"
  },
  "main": "./lib/jake.js",
  "scripts": {
    "lint": "eslint --format codeframe \"lib/**/*.js\" \"test/**/*.js\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\"",
    "test": "./bin/cli.js test",
    "test:ci": "npm run lint && ./bin/cli.js test --trace"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jakejs/jake.git"
  },
  "preferGlobal": true,
  "dependencies": {
    "filelist": "0.0.x",
    "minimatch": "3.x",
    "chalk": "0.4.x",
    "utilities": "1.0.x",
    "async": "0.9.x"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "q": "0.9.x"
  },
  "engines": {
    "node": "*"
  }
}
